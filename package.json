    "type": "git",
    "url": "git@github.com:hugoleonardodev/boilerplate-front-end.git"
  },
  "homepage": "https://github.com/hugoleonardodev/boilerplate-front-end",
  "contributors": [
    {
      "name": "Hugo Leonardo Matosinhos de Souza",
